{
  "buildCommand": "export JAVA_HOME=/vercel/.java/openjdk17 && ./gradlew clean wasmJsBrowserDistribution --no-daemon",
  "outputDirectory": "build/dist/wasmJs/productionExecutable",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}