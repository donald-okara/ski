{
  "buildCommand": "./gradlew wasmJsBrowserDistribution",
  "outputDirectory": "build/dist/wasmJs/productionExecutable",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}